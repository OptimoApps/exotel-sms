{
    "name": "satz/exotel-sms",
    "description": "Exotel SMS Laravel Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "satz",
            "email": "sathish.thi@gmail.com",
            "homepage": "https://www.optimoapps.com/"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "illuminate/support": ">=5.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Satz\\ExotelSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Satz\\ExotelSms\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Satz\\ExotelSms\\ExotelSmsServiceProvider"
            ],
            "aliases": {
                "ExotelSms": "Satz\\Exotel\\ExotelSmsFacade"
            }
        }
    }
}
