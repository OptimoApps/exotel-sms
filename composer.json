{
  "name": "optimoapps/exotel-sms",
  "description": "Exotel SMS Laravel Package",
  "keywords": [
    "sms api",
    "laravel package sms",
    "exotel sms"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/optimoapps/exotel-sms",
  "authors": [
    {
      "name": "satz",
      "email": "sathish.thi@gmail.com",
      "homepage": "https://www.optimoapps.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.5",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "4.*|5.*",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "OptimoApps\\ExotelSms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OptimoApps\\ExotelSms\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimoApps\\ExotelSms\\ExotelSmsServiceProvider"
      ],
      "aliases": {
        "ExotelSms": "OptimoApps\\ExotelSms\\ExotelSmsFacade"
      }
    }
  }
}
