{
    "name": "ssatz/exotel-sms",
    "description": "Exotel SMS Laravel Package",
    "keywords": [
        "sms api",
        "laravel package sms",
        "exotel sms"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/ssatz/exotel-sms",
    "authors": [
        {
            "name": "satz",
            "email": "sathish.thi@gmail.com",
            "homepage": "https://www.optimoapps.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": ">=5.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Ssatz\\ExotelSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ssatz\\ExotelSms\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ssatz\\ExotelSms\\ExotelSmsServiceProvider"
            ],
            "aliases": {
                "ExotelSms": "Ssatz\\ExotelSms\\ExotelSmsFacade"
            }
        }
    }
}
